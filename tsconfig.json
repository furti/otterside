{
  "compilerOptions": {
    "outDir": "./target/",
    "target": "es5",
    "module": "commonjs",
    "rootDir": "./src/web",
    "jsx": "react",
    "noEmitHelpers": true
  },
  "filesGlob": [
    "./src/**/*.ts",
    "./src/**/*.tsx"
  ],
  "files": [
    "./src/web/console/mainmenu/start.ts",
    "./src/web/scripts/BootState.ts",
    "./src/web/scripts/CommandParams.ts",
    "./src/web/scripts/Http.ts",
    "./src/web/scripts/MainMenuState.ts",
    "./src/web/scripts/MapUtils.ts",
    "./src/web/scripts/ObjectStore.ts",
    "./src/web/scripts/OttersideGame.ts",
    "./src/web/scripts/PlayState.ts",
    "./src/web/scripts/PreloadState.ts",
    "./src/web/scripts/refs.d.ts",
    "./src/web/scripts/typescript-helpers.ts",
    "./src/web/scripts/Console.tsx",
    "./src/web/scripts/InteractiveContent.tsx",
    "./src/web/scripts/Loading.tsx",
    "./src/web/scripts/MarkdownParagraph.tsx"
  ],
  "atom": {
    "rewriteTsconfig": true
  },
  "compileOnSave": false
}
