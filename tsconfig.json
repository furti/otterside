    "./src/web/scripts/PlayState.ts",
    "./src/web/scripts/PreloadState.ts",
    "./src/web/scripts/refs.d.ts",
    "./src/web/scripts/Console.tsx",
    "./src/web/scripts/InteractiveContent.tsx",
    "./src/web/scripts/Loading.tsx",
    "./src/web/scripts/MarkdownParagraph.tsx"
